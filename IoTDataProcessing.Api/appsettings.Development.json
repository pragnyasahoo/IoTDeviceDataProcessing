{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "your-tenant-id",  
    "ClientId": "your-client-id"  
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=IoTDb;Integrated Security=True;Trust Server Certificate=True"
  },
  "KeyVault": {
    "Endpoint": "https://<your-key-vault-name>.vault.azure.net/"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
