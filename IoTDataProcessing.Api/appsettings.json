{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "your-tenant-id",
    "ClientId": "your-client-id"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Initial Catalog=IoTDb;Integrated Security=True;Pooling=False;Encrypt=True;Trust Server Certificate=False"
  },
  "IoTHub": {
    "ConnectionString": "HostName=your_iot_hub.azure-devices.net;SharedAccessKeyName=iothubowner;SharedAccessKey=your_shared_access_key"
  },
  "KeyVault": {
    "Endpoint": "https://<your-key-vault-name>.vault.azure.net/"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
